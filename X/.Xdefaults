URxvt.font: xft:consolas:bold:size=12
URxvt.boldFont: xft:consolas:bold:size=12
URxvt.background: [95]#131416
URxvt.depth: 32

URxvt.letterSpace: 0
URxvt.lineSpace: -1

! interface
URxvt.saveLines:            9999
URxvt.scrollstyle:          plain
URxvt.scrollBar:            false

URxvt.urgentOnBell:         true

! clipboard
